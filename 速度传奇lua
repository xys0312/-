local lovelib = loadstring(game:HttpGet("https://raw.githubusercontent.com/synolope/lovelib/main/lovelibbeta.lua",true))() local gui = lovelib:new() local Farming = gui:newpage("Farming") Farming:addlabel("Farming") local autosteps = nil Farming:addcheckbox("Auto Steps",false,function(b) 	if autosteps then autosteps:Disconnect() autosteps = nil end 	if b then 		autosteps = game:GetService("RunService").Heartbeat:Connect(function() 			local A_1 = "collectOrb" 			local A_2 = "Red Orb" 			local A_3 = "City" 			local Event = game:GetService("ReplicatedStorage").rEvents.orbEvent 			Event:FireServer(A_1, A_2, A_3) 		end) 	end end) local collectorbs = nil Farming:addcheckbox("Auto Collect Gems",false,function(b) 	if collectorbs then collectorbs:Disconnect() collectorbs = nil end 	if b then 		collectorbs = game:GetService("RunService").Heartbeat:Connect(function() 			local A_1 = "collectOrb" local A_2 = "Gem" local A_3 = "City" local Event = game:GetService("ReplicatedStorage").rEvents.orbEvent Event:FireServer(A_1, A_2, A_3) 		end) 	end end) local collectgems = nil Farming:addcheckbox("Auto Collect Orbs",false,function(b) 	if collectgems then collectgems:Disconnect() collectgems = nil end 	if b then 		collectgems = game:GetService("RunService").Heartbeat:Connect(function() 			local A_1 = "collectOrb" local A_2 = "Yellow Orb" local A_3 = "City" local Event = game:GetService("ReplicatedStorage").rEvents.orbEvent Event:FireServer(A_1, A_2, A_3) 		end) 	end end) local rebirths = nil Farming:addcheckbox("Auto Rebirth",false,function(b) 	if rebirths then rebirths:Disconnect() rebirths = nil end 	if b then 		rebirths = game:GetService("RunService").Heartbeat:Connect(function() 			-- This script was generated by Hydroxide's RemoteSpy: https://github.com/Upbolt/Hydroxide local ohString1 = "rebirthRequest" game:GetService("ReplicatedStorage").rEvents.rebirthEvent:FireServer(ohString1) 		end) 	end end) local hoops = nil Farming:addcheckbox("Hoops Farm",false,function(b) if hoops then hoops:Disconnect() hoops = nil end if b then while true do local children = workspace.Hoops:GetChildren() for i, child in ipairs(children) do if child.Name == "Hoop" then child.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame wait(.5) end end end end end) local Crystals = gui:newpage("Crystals") Crystals:addlabel("Crystals") for _,c in pairs(game:GetService("ReplicatedStorage").crystalPrices:GetChildren()) do 	Crystals:addbutton("Buy " .. c.Name,nil,function() 		-- This script was generated by Hydroxide's RemoteSpy: https://github.com/Upbolt/Hydroxide local ohString1 = "openCrystal" local ohString2 = c.Name game:GetService("ReplicatedStorage").rEvents.openCrystalRemote:InvokeServer(ohString1, ohString2) 	end) end Farming:select()
